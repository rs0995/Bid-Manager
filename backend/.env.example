# Comma-separated API keys allowed to call the backend.
# Example: APP_API_KEYS=key_client_a,key_client_b
APP_API_KEYS=change-this-key
ADMIN_API_KEY=change-this-admin-key

# Host/port for local run
HOST=0.0.0.0
PORT=8000

# Base data folder for server-side DB and downloads
SERVER_DATA_DIR=./server_data

# Default timeout (seconds) to wait for a manual captcha answer
CAPTCHA_TIMEOUT_SECONDS=300
